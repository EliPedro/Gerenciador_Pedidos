<div class="container">
    <div class="modal fade" id="mdCliente" role="dialog"  data-keyboard="false" data-backdrop="static">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title"><strong>Novo Cliente</strong></h4>
                </div>
                <div class="modal-body">
                   <div class="form-horizontal">
                       <div class="form-group">
                            <label for="txtNome" class="control-label col-md-2">Nome</label>
                           <div class="col-md-10">
                                <input class="form-control" autofocus type="text" id="txtNome"/>
                           </div>                
                       </div>
                       <div class="form-group">
                           <label for="txtCPF" class="control-label col-md-2">CPF</label>
                           <div class="col-md-8">
                               <input min="11" max="11" maxlength="11" class="form-control" type="text" id="txtCPF"/>
                               <strong id="erroCPF"></strong>
                           </div>
                       </div>
                       <div class="form-group">
                           <label for="txtEmail" class="control-label col-md-2">Email</label>
                           <div class="col-md-8">
                               <input  class="form-control" type="email" id="txtEmail" />
                           </div>
                       </div>
                       <div class="form-group">
                           <label for="txtCEP" class="control-label col-md-2">CEP</label>
                           <div class="col-md-8">
                               <input class="form-control" type="text" id="txtCEP" />
                           </div>
                       </div>

                       <div class="form-group">
                           <label for="txtBairro" class="control-label col-md-2">Bairro</label>
                           <div class="col-md-8">
                               <strong class="form-control" id="txtBairro" readonly></strong>
                           </div>
                       </div>

                       <div class="form-group">
                           <label for="txtCidade" class="control-label col-md-2">Cidade</label>
                           <div class="col-md-8">
                               <strong class="form-control" id="txtCidade" readonly></strong>
                           </div>
                       </div>
                       <div class="form-group">
                           <label for="txtEstado" class="control-label col-md-2">Estado</label>
                           <div class="col-md-8">
                               <strong class="form-control" id="txtEstado" readonly></strong>
                           </div>
                       </div>
                       <div class="form-group">
                           <label for="txtLogradouro" class="control-label col-md-2">Logradouro</label>
                           <div class="col-md-8">
                               <strong class="form-control" id="txtLogradouro" readonly></strong>
                           </div>
                       </div>
                       <div class="form-group">
                           <label for="txtNumero" class="control-label col-md-2">Número</label>
                           <div class="col-md-2">
                               <input class="form-control" type="text" id="txtNumero" />
                           </div>
                       </div>
                   </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" value="0" id="btnCadastrarCliente">Cadastrar</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
                </div>
            </div>
        </div>
    </div>
</div>
